{
	"name": "independent-framework-fe",
	"version": "1.0.0",
	"description": "Framework-agnostic application with swappable UI layers",
	"type": "module",
	"scripts": {
		"dev:react": "vite --config ui/react/vite.config.ts",
		"build:react": "tsc && vite build --config ui/react/vite.config.ts",
		"preview:react": "vite preview --config ui/react/vite.config.ts",
		"type-check": "tsc --noEmit",
		"test": "vitest"
	},
	"keywords": [
		"framework-agnostic",
		"clean-architecture",
		"react",
		"vue",
		"angular"
	],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^20.10.6",
		"@types/react": "^18.2.46",
		"@types/react-dom": "^18.2.18",
		"@vitejs/plugin-react": "^4.2.1",
		"typescript": "^5.3.3",
		"vite": "^5.0.10",
		"vitest": "^1.1.0"
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	}
}
